<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.brcd.mapper.PersonageMessageMapper">
    <select id="queryAgentMsg" parameterType="TbAgent" resultType="TbAgent">
		SELECT company_affiliation,attribution_department,name,email,phone,mobile_phone,remark,account_type,role,last_ip,last_login_time FROM tb_agent
		        WHERE id=#{id};
    </select>

</mapper>